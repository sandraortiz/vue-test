,CAA4C,MAA5C,EAA4C;AAAtC,EAAA,KAAK,EAAC;AAAgC,CAA5C,EAA+B,QAA/B,EAAqC,CAAA,CAArC,C;;;AAIG,EAAA,KAAK,EAAC;;;;;uBAdf,mBAAA,CAqBM,KArBN,EAAA,UAAA,EAqBM,CApBJ,YAAA,CAAgE,sBAAhE,EAAgE;AAAnD,IAAA,EAAE,EAAC,QAAgD;AAAvC,IAAA,GAAG,EAAC;AAAmC,GAAhE,E;sBAAsC;AAAA,aAAY,C,UAAA,CAAZ;AAAA,K;;GAAtC,CAoBI,EAnBJ,UAmBI,EAnBA,UAmBA,EAlBJ,mBAAA,CAgBO,MAhBP,EAgBO,IAhBP,EAgBO,CAfL,mBAAA,CAGM,KAHN,EAAA,UAAA,EAGM,CAFJ,UAEI,E,gBADJ,mBAAA,CAAmE,OAAnE,EAAmE;;aAAnD,KAAA,CAAA,OAAA,CAAQ,M,GAAM,M;MAAqC;AAAnC,IAAA,IAAI,EAAC,MAA8B;AAAvB,IAAA,KAAK,EAAC;AAAiB,GAAnE,E,IAAA,E,GAAA,C,iBAAgB,KAAA,CAAA,OAAA,CAAQ,M,GACpB,CAHN,CAeK,EAVL,mBAAA,CAGM,KAHN,EAAA,UAAA,EAGM,CAFJ,UAEI,E,gBADJ,mBAAA,CAAmE,OAAnE,EAAmE;;aAAnD,KAAA,CAAA,OAAA,CAAQ,M,GAAM,M;MAAqC;AAAnC,IAAA,IAAI,EAAC,MAA8B;AAAvB,IAAA,KAAK,EAAC;AAAiB,GAAnE,E,IAAA,E,GAAA,C,iBAAgB,KAAA,CAAA,OAAA,CAAQ,M,GACpB,CAHN,CAUK,EALL,mBAAA,CAIM,KAJN,EAAA,UAAA,EAIM,CAHJ,mBAAA,CAES,QAFT,EAES;AAFA,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,cAAA,CAAA,UAAA,MAAA;AAAA,aAAU,QAAA,CAAA,WAAA,EAAV;AAAA,KAAA,EAAqB,CAAA,SAAA,CAArB,CAAA,CAEL;AAF8B,IAAA,KAAK,EAAC;AAEpC,GAFT,EAA+D,WAA/D,CAGI,CAJN,CAKK,CAhBP,CAkBI,CArBN,C","sourcesContent":["<template>\n  <div class=\"container\">\n    <router-link to=\"/about\" tag=\"button\">Inica sesion</router-link>\n    <br><br>\n    <form>\n      <div class=\"input-group mb-3\">\n        <span class=\"input-group-text\">Nombre</span>\n        <input v-model=\"usuario.nombre\" type=\"text\" class=\"form-control\" />\n      </div>\n\n      <div class=\"input-group mb-3\">\n        <span class=\"input-group-text\">Correo</span>\n        <input v-model=\"usuario.correo\" type=\"text\" class=\"form-control\" />\n      </div>\n\n      <div class=\"mt-3\">\n        <button @click.prevent=\"agregarDato()\" class=\"btn btn-primary\">\n          Guardar\n        </button>\n      </div>\n    </form>\n  \n  </div>\n</template>\n\n<script>\nimport {\n  collection,\n  getDocs,\n  addDoc,\n  deleteDoc,\n  doc,\n} from \"firebase/firestore/lite\";\nimport { db } from \"../main\";\nimport router from \"../router/index\";\n\nexport default {\n  name: \"Home\",\n  data() {\n    return {\n      usuarios: [],\n      textos: [],\n      usuario: {\n        nombre: \"\",\n        correo: \"\",\n      },\n      texto: {\n        name: \"\",\n      },\n    };\n  },\n  methods: {\n    async obtenerDatos() {\n      const querySnapshot = await getDocs(collection(db, \"usuarios\"));\n      querySnapshot.forEach((doc) => {\n        let usuario = doc.data();\n        usuario.id = doc.id;\n        this.usuarios.push(usuario);\n        console.log(usuario);\n      });\n    },\n    async obtenerTextos() {\n      const querySnapshot = await getDocs(collection(db, \"textos\"));\n      querySnapshot.forEach((doc) => {\n        let texto = doc.data();\n        texto.id = doc.id;\n        this.textos.push(texto);\n        console.log(texto);\n      });\n    },\n    async agregarDato() {\n      const docRef = await addDoc(collection(db, \"usuarios\"), {\n        nombre: this.usuario.nombre,\n        correo: this.usuario.correo,\n      })\n        .then(() => {\n          router.go(\"/\");\n          console.log(\"Documento añadido\");\n        })\n        .catch(function (error) {\n          console.error(\"Error al añadir el documento: \", error);\n        });\n    },\n    // DELETE / ELIMINAR / BORRAR\n    async eliminarDato(id) {\n      await deleteDoc(doc(db, \"usuarios\", id));\n      router.go(\"/\");\n    },\n  },\n  mounted() {\n    this.obtenerDatos();\n    this.obtenerTextos();\n  },\n};\n</script>\n"],"sourceRoot":""}]}