,EAAR,GAAa,GAAG,CAAC,EAAjB;;AACA,kBAAA,KAAI,CAAC,QAAL,CAAc,IAAd,CAAmB,OAAnB;;AACA,kBAAA,OAAO,CAAC,GAAR,CAAY,OAAZ;AACD,iBALD;;AAFmB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQpB,KATM;AAUD,IAAA,aAVC,2BAUe;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACQ,OAAO,CAAC,UAAU,CAAC,EAAD,EAAK,QAAL,CAAX,CADf;;AAAA;AACd,gBAAA,aADc;AAEpB,gBAAA,aAAa,CAAC,OAAd,CAAsB,UAAC,GAAD,EAAS;AAC7B,sBAAI,KAAI,GAAI,GAAG,CAAC,IAAJ,EAAZ;AACA,kBAAA,KAAK,CAAC,EAAN,GAAW,GAAG,CAAC,EAAf;;AACA,kBAAA,MAAI,CAAC,MAAL,CAAY,IAAZ,CAAiB,KAAjB;;AACA,kBAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,iBALD;;AAFoB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQrB,KAlBM;AAmBD,IAAA,WAnBC,yBAmBa;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACG,MAAM,CAAC,UAAU,CAAC,EAAD,EAAK,UAAL,CAAX,EAA6B;AACtD,kBAAA,MAAM,EAAE,MAAI,CAAC,OAAL,CAAa,MADiC;AAEtD,kBAAA,MAAM,EAAE,MAAI,CAAC,OAAL,CAAa;AAFiC,iBAA7B,CAAN,CAIlB,IAJkB,CAIb,YAAM;AACV,kBAAA,MAAM,CAAC,EAAP,CAAU,GAAV;AACA,kBAAA,OAAO,CAAC,GAAR,CAAY,mBAAZ;AACD,iBAPkB,EAQlB,KARkB,CAQZ,UAAU,KAAV,EAAiB;AACtB,kBAAA,OAAO,CAAC,KAAR,CAAc,gCAAd,EAAgD,KAAhD;AACD,iBAVkB,CADH;;AAAA;AACZ,gBAAA,MADY;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYnB,KA/BM;AAgCP;AACM,IAAA,YAjCC,wBAiCY,EAjCZ,EAiCgB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACf,SAAS,CAAC,GAAG,CAAC,EAAD,EAAK,UAAL,EAAiB,EAAjB,CAAJ,CADM;;AAAA;AAErB,gBAAA,MAAM,CAAC,EAAP,CAAU,GAAV;;AAFqB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAGtB;AApCM,GAfI;AAqDb,EAAA,OArDa,qBAqDH;AACR,SAAK,YAAL;AACA,SAAK,aAAL;AACD;AAxDY,CAAf","sourcesContent":["<template>\n  <div class=\"container\">\n    <form>\n      <div class=\"input-group mb-3\">\n        <span class=\"input-group-text\">Nombre</span>\n        <input v-model=\"usuario.nombre\" type=\"text\" class=\"form-control\" />\n      </div>\n\n      <div class=\"input-group mb-3\">\n        <span class=\"input-group-text\">Correo</span>\n        <input v-model=\"usuario.correo\" type=\"text\" class=\"form-control\" />\n      </div>\n\n      <div class=\"mt-3\">\n        <button @click.prevent=\"agregarDato()\" class=\"btn btn-primary\">\n          Guardar\n        </button>\n      </div>\n    </form>\n    <table class=\"table\">\n      <thead>\n        <tr>\n          <th scope=\"col\">id</th>\n          <th scope=\"col\">Nombre</th>\n          <th scope=\"col\">Correo</th>\n          <th scope=\"col\">Eliminar</th>\n        </tr>\n      </thead>\n      <tbody>\n        <tr v-for=\"(item, index) in usuarios\" :key=\"index\">\n          <th scope=\"row\">{{ index }}</th>\n          <td>{{ item.nombre }}</td>\n          <td>{{ item.correo }}</td>\n          <td>\n            <button\n              @click.prevent=\"eliminarDato(item.id)\"\n              class=\"btn btn-danger\"\n            >\n              Eliminar\n            </button>\n          </td>\n        </tr>\n      </tbody>\n    </table>\n    <table class=\"table\">\n      <thead>\n        <tr>\n          <th scope=\"col\">id</th>\n          <th scope=\"col\">Nombre</th>\n          <th scope=\"col\">Correo</th>\n          <th scope=\"col\">Eliminar</th>\n        </tr>\n      </thead>\n      <tbody>\n        <tr v-for=\"(item, index) in textos\" :key=\"index\">\n          <th scope=\"row\">{{ index }}</th>\n          <td>{{ item.name }}</td>\n          <!-- <td>{{ item.correo }}</td> -->\n          <td>\n            <button\n              @click.prevent=\"eliminarDato(item.id)\"\n              class=\"btn btn-danger\"\n            >\n              Eliminar\n            </button>\n          </td>\n        </tr>\n      </tbody>\n    </table>\n  </div>\n</template>\n\n<script>\nimport {\n  collection,\n  getDocs,\n  addDoc,\n  deleteDoc,\n  doc,\n} from \"firebase/firestore/lite\";\nimport { db  , analytics } from \"../main\";\nimport router from \"../router/index\";\n\nexport default {\n  name: \"Home\",\n  data() {\n    return {\n      usuarios: [],\n      textos: [],\n      usuario: {\n        nombre: \"\",\n        correo: \"\",\n      },\n      texto:{\n        name:\"\",\n      }\n    };\n  },\n  methods: {\n    async obtenerDatos() {\n      const querySnapshot = await getDocs(collection(db, \"usuarios\"));\n      querySnapshot.forEach((doc) => {\n        let usuario = doc.data();\n        usuario.id = doc.id;\n        this.usuarios.push(usuario);\n        console.log(usuario);\n      });\n    },\n    async obtenerTextos() {\n      const querySnapshot = await getDocs(collection(db, \"textos\"));\n      querySnapshot.forEach((doc) => {\n        let texto = doc.data();\n        texto.id = doc.id;\n        this.textos.push(texto);\n        console.log(texto);\n      });\n    },\n    async agregarDato() {\n      const docRef = await addDoc(collection(db, \"usuarios\"), {\n        nombre: this.usuario.nombre,\n        correo: this.usuario.correo,\n      })\n        .then(() => {\n          router.go(\"/\");\n          console.log(\"Documento añadido\");\n        })\n        .catch(function (error) {\n          console.error(\"Error al añadir el documento: \", error);\n        });\n    },\n    // DELETE / ELIMINAR / BORRAR\n    async eliminarDato(id) {\n      await deleteDoc(doc(db, \"usuarios\", id));\n      router.go(\"/\");\n    },\n  },\n  mounted() {\n    this.obtenerDatos();\n    this.obtenerTextos();\n  },\n};\n</script>\n"],"sourceRoot":""}]}